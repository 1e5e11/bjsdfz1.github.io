            mySwiper6.onResize();
        }
        catch (e) { }

        try {
            if (pagew > 1200) {
                mySwiper5.params.slidesPerView = 7;
            }
            else {
                mySwiper5.params.slidesPerView = 3;
            }
            mySwiper5.update();
            mySwiper5.onResize();
